public class Groceries
{
  public static void main(String[] args)
  {
  GroceryList list=new GroceryList();
 
  list.add(new GroceryItem("Oreos", 3, 3.00));
  list.add(new GroceryItem("Oranges", 7, 2.00));
  list.add(new GroceryItem("boxes", 1, 1000000000.00));
  list.add(new GroceryItem("pencil", 3, .25));
  list.add(new GroceryItem("carrots", 8 , 3.44));
  System.out.println("The list is" +list.toString());
  System.out.println("The total cost is" +list.getTotalCost());
  }
}
